pragma solidity ^0.8.0;

import { IFeeCollector } from "./IFeeCollector.sol";
import { Ownable } from "./Ownable.sol";
import { IFees } from "./IFees.sol";

abstract contract FeeCollector is IFeeCollector, Ownable {
  IFees internal _fees;

  function _setFeesContract(address contractAddress_) internal {
    _fees = IFees(contractAddress_);
  }

  function setFeesContract(address contractAddress_) external override onlyOwner {
    _setFeesContract(contractAddress_);
  }
}
